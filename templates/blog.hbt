{{> head }}
<body>
  <div class="container">
    {{> navigation }}
    <div class="page">
      <main>
        {{{contents}}}
        <div class="posts">
          {{ log pagination }}
        {{#each collections.blog }}
          <h2><a href="/{{ this.path }}">{{ this.headline }}</a><br>{{ longDate this.date }}</h2>
        {{/each}}
        </div>
      </main>
    </div>
    {{> footer }}
  </div>
  {{> scripts }}
</body>
</html>
